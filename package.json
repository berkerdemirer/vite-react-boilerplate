{
  "name": "vite-react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint --fix .",
    "generate-api": "npx openapi-typescript http://localhost:3000/api/v1/openapi -o src/lib/schema.d.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@radix-ui/react-dialog": "^1.1.12",
    "@radix-ui/react-label": "^2.1.5",
    "@radix-ui/react-navigation-menu": "^1.2.11",
    "@radix-ui/react-select": "^2.2.3",
    "@radix-ui/react-slot": "^1.2.1",
    "@radix-ui/react-visually-hidden": "^1.2.1",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-query-devtools": "^5.75.2",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "ldrs": "^1.1.7",
    "lucide-react": "^0.507.0",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "openapi-fetch": "^0.13.7",
    "openapi-react-query": "^0.4.1",
    "openapi-typescript-fetch": "^2.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.2",
    "react-i18next": "^15.5.1",
    "react-router": "^7.5.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tw-animate-css": "^1.2.9",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.7.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.2",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --check"
    ]
  }
}
